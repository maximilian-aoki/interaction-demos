{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "import/no-extraneous-dependencies": "off",
    "no-shadow": "off",
    "no-unused-vars": "off",
    "no-param-reassign": "off",
    "no-restricted-syntax": "off"
  }
}
